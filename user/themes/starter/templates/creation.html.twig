{% embed 'partials/base.html.twig' %}

{% block content %}

<h1>{{ page.title }}</h1>

<section class="content creation">
	{{ content }}

	{% if page.header.subTitle !="" %}
	<div class="highlight">
		<h2>Projet mis en avant</h2>
		<h3><a href="{{page.header.link}}">{{page.header.subTitle}}</a></h3>
		{{page.header.highlight|markdown}}
		<a href="{{page.header.link}}" class="read-more">En savoir plus</a>
	</div>
	{% endif %}

</section>

{% endblock %}

{% endembed %}
